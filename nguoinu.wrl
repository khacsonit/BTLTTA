#taytrai
DEF	taytrai Transform {
	translation	0 1.3 0 
	children [
		Transform {
			translation	0 -1.3 0
			children [
				Transform {
			translation	-1.5 0.3 0
			children [
				Shape {
					appearance	Appearance {
						material Material {
							diffuseColor  182 182 182  
						}
					}
					geometry Cylinder {
						radius 0.18
						height 2
					}
				}
				Transform {
					translation	0 -1.1 0
					children [
						Shape {
							appearance	Appearance {
								material Material {
									diffuseColor  182 182 182  
								}
							}
							geometry Sphere	{
								radius 0.2

							}
						}
					]
				}
				
			]
		}
			]
		}
		
	DEF	ortaytrai OrientationInterpolator {
			key	[0 0.33 0.66 0.85 1]
			keyValue [
				1 0 0 0,
				1 0 0 -0.3,
				1 0 0 0,
				1 0 0 0.3	,
				1 0 0 0
			]
		}	
	]
} 
DEF	timetaychan TimeSensor {
			cycleInterval 2
			enabled	TRUE
			loop TRUE


		}
ROUTE timetaychan.fraction_changed TO ortaytrai.set_fraction
ROUTE ortaytrai.value_changed  TO taytrai.rotation
