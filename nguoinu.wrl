DEF	tocnu Transform {
	translation	0 2.2 0.1
	scale 0.8 1 1
	children [
		Shape {
			appearance	Appearance {
				material Material {
					diffuseColor  0 0 0 
				}
			}
			
			
			
			geometry Extrusion {
				endCap TRUE
				beginCap TRUE
				solid FALSE
				creaseAngle	1
				crossSection [
					-1 0,
					
					-0.9 -0.43589 ,
					-0.8 -0.6 ,
					-0.7 -0.714143 ,

					-0.580865 -0.814 ,
					-0.405714 -0.914 ,


					0 -1 ,
					0.43589 -0.9 ,
					0.6 -0.8  ,
					0.714143 -0.7  ,

					0.814 -0.580865  ,
					0.914 -0.405714  ,

					1 0,
					-1 0
				]
				spine [
					0 0 0,
					0 1 0,
				]
				scale [
					1 1,
					1 1
				]
			}
		}
	]
}
