DEF QuaTao Transform {
 translation 0 -3 0
 children [
  Shape {
   appearance    Appearance {
    material Material {
     diffuseColor   1 0 0}
	 texture ImageTexture {
		 repeatS TRUE
		 repeatT TRUE
		url	"mauquatao.png"
}


    }
    #hình học
    geometry Extrusion {
     #toa do tao mat phang day (dung toa do x,z)
     crossSection  [
      1 0, 0.8 -0.5, 0.5 -0.8,
      0 -1, -0.5 -0.8, -0.8 -0.5,
      -1 0, -0.8 0.5, -0.5 0.8,
      0 1, 0.5 0.8,0.8 0.5,
      1 0
     ]
     #duong xuong song (toa do y)
     spine [ 0 0.4 0,  0 0.2 0, 0 0 0, 0 0 0,  0 0.5 0,  0 1.2 0,  0 2 0,  0 2.5 0, 0 3 0, 0 3.5 0, 0 4 0, 0 4 0 0 3.5 0, 0 3 0]
     #ti le mat phang so mat phang ban dau  (toa do x)
     scale [ 0 0,  0.5 0.5, 0.8 0.8, 1.3 1.3, 2 2,  2.4 2.4, 2.6 2.6, 2.7 2.7,  2.7 2.7, 2.5 2.5, 2.1 2.1, 1.6 1.6, 1.1 1.1, 0.05 0.05]
     solid FALSE
     creaseAngle 1
     beginCap TRUE
     endCap TRUE
    }
   }
  ]
 }
